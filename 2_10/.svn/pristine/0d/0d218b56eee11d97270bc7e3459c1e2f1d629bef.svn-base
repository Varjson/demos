}

.hot-ad li img {
    width: 399px;
    height: 200px;
}

.hot-ad li a {
    background: rgba(0, 0, 0, 0.6);
    display: none;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    color: #fff;
    padding-top: 65px;
    text-align: center;
    font-size: 18px;
    line-height: 30px;
}

.hot-ad li a span {
