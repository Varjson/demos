/*生活*/

.live-fade {
    margin: 20px auto 50px;
    border: 1px solid #d3d8de;
    background: #fff;
    position: relative;
    padding-left: 235px;
}

.live-fade .hd {
    position: absolute;
    width: 185px;
    height: 100%;
    top: 0;
    left: 0;
    background: #e8f5fe;
}

.live-fade .hd p {
    cursor: pointer;
    height: 105px;
    border-bottom: 1px solid #d1e1f0;
    line-height: 104px;
    font-size: 16px;
    color: #333;
}

.live-fade .hd p.on {
    background: #fff;
}

.live-fade .hd p img {
    margin: 0 12px 0 21px;
    vertical-align: middle;
}

.live-fade .bd {
    min-height: 570px;
    padding: 30px 50px 30px 0;
}

.live-fade .bd li {
    display: none;
}

.live-fade .bd .h {
    line-height: 42px;
    height: 42px;
    font-size: 16px;
    border-bottom: 1px solid #b6c6d3;
}

.live-fade .bd .h .fl {
    color: #45a136;
}

.live-fade .bd .h .fl img {
    vertical-align: middle;
    margin-right: 10px;
}

.live-fade .bd .h .fr a {
    color: #666;
    display: inline-block;
    margin-left: 30px;
    height: 42px;
}

.live-fade .bd .h .fr a.on {
    color: #ff710b;
    border-bottom: 1px solid #ff710b;
    background: url(../images/u-y.png) no-repeat center bottom;
}

.live-form {
    padding-top: 30px;
}

.live-form .live-item {
    font-size: 14px;
    color: #666;
    min-height: 38px;
    position: relative;
    padding-left: 82px;
    margin-top: 20px;
}

.live-form .live-item>span {
    position: absolute;
    height: 38px;
    line-height: 38px;
    width: 82px;
    top: 0;
    left: 0;
}

.live-form .live-item p {
    width: 500px;
}

.live-form .live-item input+img {
    margin-left: 10px;
    cursor: pointer;
}

.live-form .live-item input[type=text],.live-form .live-item select {
    width: 340px;
    height: 38px;
    border: 1px solid #dedede;
    padding: 0 50px 0 10px;
    line-height: normal;
    background: url(../images/live6.png) no-repeat 412px center;
}

.live-form .live-item select{
width:95px; padding-right:5px; margin-right:2px;
}

.live-form .live-item .recharge span {
    display: inline-block;
    width: 95px;
    height: 38px;
    text-align: center;
    line-height: 38px;
    border: 1px solid #dedede;
    float: left;
    margin: 0 20px 20px 0;
    cursor: pointer;
}

.live-form .live-item .recharge span.on {
    border-color: #e3393a;
    background: url(../images/check.png) no-repeat right bottom;
}

.live-form .live-item .money font {
    font-size: 25px;
    color: #e5383a;
    display: inline-block;
    line-height: 40px;
}

.live-form .live-btn input {
    width: 138px;
    height: 38px;
    line-height: 36px;
    text-align: center;
    font-size: 18px;
    color: #fff;
    font-weight: bold;
    background: #c91723;
    border-radius: 5px;
    margin-right: 20px;
    cursor: pointer;
}

.live-form .live-item .notice {
    color: #005ea8;
    line-height: 15px;
    cursor: pointer;
}

.live-form .live-item .notice img {
    vertical-align: -3px;
    margin-right: 5px;
}

.live-form .live-item .rate {
    margin-top: -20px;
}

.live-form .live-item .rate>p {
    padding-bottom: 5px;
}

.live-form .live-item .rate p {
    padding-left: 10px;
    font-size: 14px;
    color: #333;
    line-height: 20px;
}

.live-form .live-item .rate-item {
    border: 1px solid #dedede;
    margin-bottom: 10px;
    width: 441px;
}

.live-form .live-item .rate-item p {
    font-size: 12px;
    color: #999;
}

.live-form .live-item .rate-item p span {
    font-size: 14px;
    color: #333;
    padding-right: 10px;
}

.live-form .live-item .rate-item a {
    float: right;
    width: 56px;
    height: 24px;
    line-height: 24px;
    text-align: center;
    font-size: 16px;
    color: #ca1622;
    border: 1px solid #ca1622;
    border-radius: 2px;
    margin: 10px;
}


/*新闻*/

.news-content {
    margin: 50px auto 60px;
    width: 1090px;
    background: #fff;
    padding: 45px 35px 45px 30px;
    box-shadow: 0 0 10px rgba(4, 0, 0, 0.2)
}

.news-menu {
    width: 190px;
    color: #666;
    font-size: 14px;
    background: #fff;
}

.news-menu-list>li p {
    line-height: 45px;
    color: #c81622;
    padding-left: 65px;
}

.news-menu-list>li p img {
    vertical-align: middle;
    margin: 0 10px 0 20px;
}

.news-sub-list {
    padding: 0 12px;
}

.news-sub-list>li {
    line-height: 35px;
    text-align: center;
    font-size: 14px;
}

.news-sub-list>li:last-child {
    border: none;
}

.news-sub-list a {
    color: #000;
}

.news-sub-list li.act a,
.news-sub-list li:hover a {
    color: #ca1622;
    text-decoration: underline;
}

.news-con {
    width: 805px;
    min-height: 800px;
    border: 1px solid #e8e8e8;
    padding-bottom: 30px;
}

.news-con .title {
    background: #f5f5f5;
    line-height: 42px;
    padding: 0 30px;
    font-size: 14px;
    color: #000;
}

.news-list {
    padding: 10px 10px 10px 30px;
    font-size: 14px;
    color: #666;
    line-height: 22px;
}

.news-list li {
    border-bottom: 1px dashed #ededed;
}

.news-list .p1 a {
    color: #000;
}

.news-list .p1,
.news-list .p3 {
    padding: 6px 0;
}

.news-detail {
    padding: 25px 20px 40px;
}

.news-head {
    padding-bottom: 10px;
    border-bottom: 1px solid #f1f1f1;
}

.news-head .p1 {
    font-size: 20px;
    line-height: 30px;
    padding: 0 5px;
    text-align: center;
}

.news-head .p2 {
    text-align: center;
    color: #808080;
    line-height: 15px;
    padding: 10px 0;
}

.news-head .p2 span {
    display: inline-block;
    padding: 0 8px;
    border-right: 1px solid #d3d3d3;
}

.news-head .p2 span:last-child {
    border: none;
}

.news-tip {
    padding: 10px 0;
    line-height: 22px;
    border-bottom: 1px solid #f1f1f1;
}

.news-tip span {
    color: #f30503;
}

.news-text {
    padding: 15px 0 30px;
    border-bottom: 1px solid #f1f1f1;
}
.news-text img{ width:100%}

.news-text pre {
    word-break: break-all;
    word-wrap: break-word;
    white-space: pre-wrap;
    color: #666666;
    line-height: 20px;
}

.news-ctl .p1 {
    text-align: right;
    padding-top: 10px;
}

.news-ctl .p1 a {
    padding-right: 5px;
}

.news-ctl p {
    line-height: 22px;
    color: #333;
}

.news-ctl p a {
    color: #666;
}







