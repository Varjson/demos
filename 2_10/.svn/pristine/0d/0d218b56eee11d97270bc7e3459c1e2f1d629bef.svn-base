/*首页banner*/

.head {
    border-bottom: none;
}

body {
    background: #f6f5fb;
}


/*内容*/

.content.bg1 {
    background: url(../images/content-bg.png) no-repeat right top;
}

.index-title {
    padding: 35px 0 10px 0;
}

.index-title img {
    display: block;
    margin: 0 auto;
}

.content1 .fl {
    width: 840px;
}

.content1 .fl ul {
    width: 860px;
}

.content1 .fl li {
    position: relative;
    width: 268px;
    height: 200px;
    margin-right: 18px;
    margin-top: 18px;
    padding: 15px 25px;
    float: left;
    color: #fff;
    background: url() no-repeat right bottom #c91723
}

.content1 .fl li.max {
    width: 554px;
}

.content1 .fl li.bg1 {
    background-image: url(../images/ser-bg1.png);
}

.content1 .fl li.bg2 {
    background-image: url(../images/ser-bg2.png);
}

.content1 .fl li.bg3 {
    background-image: url(../images/ser-bg3.png);
}

.content1 .fl li.bg4 {
    background-image: url(../images/ser-bg4.png);
}

.content1 .fl li.bg5 {
    background-image: url(../images/ser-bg5.png);
}

.content1 .fl li .p1 {
    font-size: 32px;
    border-bottom: 1px solid #fff;
    text-align: center;
    line-height: 44px;
}

.content1 .fl li .p2 {
    font-size: 16px;
    line-height: 24px;
    padding-top: 5px;
}

.content1 .fl li.max .p1 {
    display: inline-block;
    width: 44px;
    border: none;
    border-right: 1px solid #fff;
    height: 160px;
    padding: 35px 0;
    margin-right: 20px;
}

.content1 .fl li.max .p2 {
    display: inline-block;
    vertical-align: top;
    padding-top: 40px;
}

.content1 .fl li.max a {
    bottom: 45px;
    right: 20px;
    left: auto;
}

.content1 .fl li a {
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
    bottom: 10px;
    width: 130px;
    height: 36px;
    line-height: 36px;
    text-align: center;
    text-align: center;
    font-size: 16px;
    color: #4c4c4c;
    background: rgba(255, 255, 255, 0.9);
}

.content1 .fr {
    width: 326px;
    height: 418px;
    margin-right: 16px;
    margin-top: 18px;
    position: relative;
}

.content1 .fr .con {
    width: 326px;
    height: 418px;
    background: #fff;
    padding: 10px 18px;
    position: relative;
}

.content1 .fr .border {
    position: absolute;
    width: 326px;
    height: 418px;
    border: 3px solid #666;
    top: -10px;
    right: -10px;
    z-index: 0;
}

.content1 .fr p {
    font-size: 18px;
    color: #4c4c4c;
    line-height: 32px;
    border-bottom: 1px solid #b3b3b3;
}

.content1 .fr table {
    width: 100%;
    line-height: 42px;
    font-size: 14px;
    color: #000;
}

.content1 .fr table th {
    text-align: left;
    border-bottom: 1px dashed #b7b7b7;
    font-weight: bold;
	width:33.3%
}

.content1 .fr table td {
    color: #666;
    border-bottom: 1px dashed #b7b7b7;
}

.content1 .fr a {
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
    bottom: 10px;
    width: 240px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 16px;
    color: #fff;
    background: #ff3342;
}

.daigou-list {}

.daigou-list li {
    width: 238px;
    height: 398px;
    float: left;
    position: relative;
    margin-right: 2px;
}

.daigou-list li>img {
    width: 238px;
    height: 398px;
}

.daigou-list li .text {
    position: absolute;
    bottom: 0;
    width: 160px;
    left: 0;
    right: 0;
    margin: auto;
    background: #fff;
    border-bottom: 4px solid #ca1622;
    -webkit-transition: all 600ms ease 0s;
    -moz-transition: all 600ms ease 0s;
    transition: all 600ms ease 0s;
}

.daigou-list li .text>p {
    line-height: 42px;
    color: #ca1622;
    text-align: center;
    font-size: 22px;
}

.daigou-list li .text-link {
    padding: 0 10px;
    text-align: center;
    display: none;
    overflow: hidden;
}

.daigou-list li .text-link p a {
    display: inline-block;
    font-size: 14px;
    color: #666;
    margin-right: 5px;
    line-height: 22px;
}

.daigou-list li .text-link .p1 {
    text-align: left;
}

.daigou-list li .text-link>a {
    width: 105px;
    height: 30px;
    display: block;
    margin: 15px auto;
    border: 1px solid #999;
    line-height: 28px;
    color: #ca1622;
}

.daigou-list li:hover .text {
    padding-top: 10px;
}

.daigou-tit {
    padding: 15px;
}

.daigou-link a {
    padding-left: 2px;
}

.content.bg2 {
    background: url(../images/content-bg2.jpg) no-repeat right 60px;
}

.content3 .poab {
    top: 185px;
    left: 0;
}

.content3 .fr a {
    position: absolute;
    left: 135px;
    bottom: 40px;
    width: 110px;
    height: 36px;
    line-height: 36px;
    text-align: center;
    color: #fff;
    background: rgba(0, 0, 0, 0.8)
}

.content4 ul {
    margin-top: 18px;
}

.content4 li {
    float: left;
    width: 300px;
    height: 205px;
    position: relative;
    background: #fff;
}

.content4 li img {
    position: absolute;
    right: 0;
    top: 0;
}

.content5 ul {
    padding: 18px 0 0 20px;
}

.content5 li {
    width: 278px;
    float: left;
    margin-right: 17px;
    color: #333;
}

.content5 li .p1 {
    text-align: center;
    font-size: 16px;
    font-weight: bold;
    line-height: 40px;
}

.content5 li .p1 a {
    color: #333;
}

.content5 li .img img {
    height: 155px;
    width: 278px;
}

.content5 li .head-img {
    padding: 10px 0;
    line-height: 38px;
}

.content5 li .head-img img {
    width: 38px;
    height: 38px;
    border-radius: 50%;
    vertical-align: middle;
    margin-right: 15px;
    font-size: 14px;
}

.content5 li .text {
    line-height: 24px;
}

.content5 li .ctl {
    color: #999999;
    padding: 10px 10px 10px 0;
}

.content5 li .ctl span {
    margin-left: 20px;
    display: inline-block;
}

span.heart {
    padding-left: 15px;
    background: url(../images/heart.png) no-repeat left center;
}

span.mes {
    padding-left: 15px;
    background: url(../images/mes.png) no-repeat left center;
}

.shaidan {
    padding: 0 40px;
    margin: 70px auto 80px;
    position: relative;
}

.shaidan li {
    width: 200px;
    height: 220px;
    float: left;
    border: 1px solid #d9d9d9;
    margin-left: 20px;
    padding: 8px 18px;
    background: #fff;
    text-align: center;
    font-size: 14px;
    -webkit-transition: all 600ms ease 0s;
    -moz-transition: all 600ms ease 0s;
    transition: all 600ms ease 0s;
    position: relative;
}

.shaidan li:hover {
    z-index: 2;
    border-color: #fff;
    transform: scale(1.35);
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.4);
}

.shaidan li .img {
    width: 167px;
    height: 120px;
    line-height: 120px;
    text-align: center;
}

.shaidan li .img img {
    max-width: 167px;
    max-height: 120px;
    vertical-align: middle;
}

.shaidan li .p1 {
    color: #000;
    padding: 10px 0 0;
}

.shaidan li .p2 {
    color: #999;
}

.shaidan li .p3 {
    text-align: left;
    color: #666;
    padding-top: 10px;
}

.content6 span {
    position: absolute;
    left: 0px;
    width: 35px;
    height: 65px;
    padding: 20px 10px;
    background: #d9d9d9;
    margin: auto;
    top: 0;
    bottom: 0;
    cursor: pointer;
}

.content6 span.right {
    left: auto;
    right: 0px;
}


/*代购*/

.hot-daigou .index-title {
    padding: 40px 0 15px 0;
    position: relative;
}

.hot-daigou .index-title a {
    font-size: 18px;
    color: #333;
    position: absolute;
    bottom: 13px;
    right: 0;
}

.hot-daigou .banner-ctl,
.hot-daigou.banner-wr {
    height: 425px;
}

.hot-ad li {
    width: 399px;
