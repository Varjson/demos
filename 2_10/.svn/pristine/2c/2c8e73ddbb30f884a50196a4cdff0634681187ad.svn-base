html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
summary,
time,
mark,
audio,
video,
input,
button {
    margin: 0;
    padding: 0;
    border: none;
    outline: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
    box-sizing: border-box;
    font-family: "微软雅黑"
}

textarea,
input,
button,
select,
textarea:focus,
input:focus,
button:focus,
select:focus {
    border: none;
    outline: none;
    background: transparent;
}

li {
    list-style: none;
}

a,
a[href] {
    text-decoration: none;
}

body {
    font-size: 12px;
    font-family: "微软雅黑"
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.poab {
    position: absolute;
}

.pore {
    position: relative;
}

.pofix {
    position: fixed;
}

.clear {
    clear: both;
}

.clear::after {
    content: '';
    display: block;
    clear: both;
}

.hide {
    display: none !important;
}

font {
    color: #ca1622;
}

.pb50 {
    padding-bottom: 50px;
}

body.fa {
    background: #fafafa;
}


/*头部样式*/

.container {
    width: 1200px;
    margin: 0 auto;
}

.head {
    min-width: 1200px;
    margin: 0 auto;
    border-bottom: 1px solid #ca1622;
    background: #fff;
}

.head-nav-wr {
    min-width: 1200px;
    height: 35px;
    line-height: 35px;
    background: #e5e5e5;
}

.head-nav {
    color: #666;
}

.head-nav a {
    color: #666;
}

.head-nav .fl a {
    color: #ca1622;
    padding: 0 5px;
}

.head-nav .fr a {
    display: inline-block;
    height: 35px;
    line-height: 35px;
    padding: 0 7px;
}

.head-nav .fr span {
    padding: 0 8px;
    font-size: 14px;
}

.head-nav .fr a.act {
    border: 1px solid #ccc;
    background: #fff;
    border-bottom: none;
    padding: 0 6px;
}

.logo-nav {
    height: 115px;
    line-height: 115px;
}

.logo-nav .logo {
    vertical-align: middle;
    height: 62px;
}

.search-wr {
    line-height: 31px;
    width: 620px;
    padding-top: 42px;
}

.search {
    border: 2px solid #ca1622;
    height: 35px;
    color: #808080;
    font-size: 13px;
}

.search-m {
    width: 78px;
    background: #e5e5e5;
    position: relative;
    cursor: pointer;
    z-index: 999
}

.search-m>span {
    display: inline-block;
    padding-left: 15px;
    letter-spacing: 2px;
}

.search-m>img {
    vertical-align: 3px;
    margin-left: 3px;
}

.search-m .s-menu {
    position: absolute;
    top: 32px;
    left: -2px;
    width: 80px;
    background: #e5e5e5;
}

.search-m .s-menu li {
    cursor: pointer;
    padding-left: 15px;
}

.search-m .s-menu li:hover {
    background: #ca1622;
    color: #fff;
}

.search-tip {
    color: #8c8c8c;
    padding-left: 15px;
    line-height: 26px;
}

.search input[type=text] {
    width: 420px;
    display: block;
    height: 31px;
    line-height: normal;
    padding: 0 20px;
    ;
    background: transparent;
}

.search button {
    width: 118px;
    height: 31px;
    text-align: center;
    color: #fff;
    cursor: pointer;
    background: #ca1622;
    border: none;
}

.link-nav {
    height: 34px;
    line-height: 34px;
    position: relative;
}

.link-nav>div>a {
    color: #666;
    font-size: 16px;
    display: block;
    float: left;
}

.link-nav .fl>a {
    margin-right: 50px;
    height: 34px;
}

.link-nav .fr a {
    margin-left: 40px;
}

.link-nav .fl a.act {
    background: #ca1622;
    color: #fff;
    text-align: center;
    width: 180px;
}


/*首页风格导航*/

.link-nav-wr {
    background: #dcdcdc;
    min-width: 1200px;
    height: 40px;
    line-height: 40px;
}

.link-nav-blant {
    background: #fff;
}

.link-nav-i a {
    display: inline-block;
    width: 150px;
    float: left;
    text-align: center;
    font-size: 16px;
    color: #666666;
}

.link-nav-i a.act,
.link-nav-i a:hover {
    background: #d92028;
    color: #fff;
}


/*头部导航菜单*/

.nav-menu {
    width: 100%;
    top: 35px;
    left: 0;
    height: auto;
    min-height: 180px;
    padding: 20px 18px 20px 0;
    background: #fff;
    border: 1px solid #d2d2d2;
    z-index: 1000;
    display: none;
}

.nav-menu li {
    float: left;
    width: 25%;
    padding-left: 18px;
    line-height: 26px;
    color: #666;
}

.nav-menu li+li {
    border-left: 1px solid #d2d2d2;
}

.nav-menu li>p {
    font-size: 14px;
    color: #666;
    font-weight: bold;
    line-height: 40px;
}

.nav-menu li a {
    width: 60px;
    margin-right: 8px;
    float: left;
    color: #666;
}


/*默认banner*/

.banner-wr {
    overflow: hidden;
    height: 520px;
}

.banner-ctl {
    width: 1200px;
    margin: 0 auto;
    height: 520px;
    position: relative;
}

.banner-ctl span {
    position: absolute;
    left: 100px;
    width: 30px;
    height: 50px;
    padding: 15px 10px;
    background: #333;
    margin: auto;
    top: -36px;
    bottom: 0;
    cursor: pointer;
}

.banner-ctl span.right {
    left: auto;
    right: 100px;
}

.banner {
    width: 1920px;
    left: 50%;
    margin-left: -960px;
}

.banner li {
    width: 1920px;
    float: left;
}

.banner li img {
    margin: 0 auto;
    display: block;
}


/*底部公共*/

.foot-wr {
    background: #e5e5e5;
    min-width: 1200px;
}

.foot .partner {
    padding: 38px 0 20px;
}

.foot .partner img {
    width: 89px;
    margin-bottom: 9px;
}

.foot .partner img+img {
    margin-left: 13px;
}

.foot-link {
    padding-bottom: 55px;
    border-bottom: 1px solid #ccc;
}

.foot-link li {
    float: left;
    width: 150px;
    font-size: 14px;
}

.foot-link li p {
    color: #000;
    padding-bottom: 20px;
}

.foot-link li a {
    display: block;
    line-height: 25px;
    color: #a6a6a6;
}

.foot-link li.l1 {
    font-size: 16px;
    line-height: 24px;
    width: 200px;
    margin-left: 90px;
}

.foot-link li.l1 i {
    color: #737989;
}

.foot-link li.l1 p {
    color: #898fa2;
    padding-bottom: 15px;
}

.foot-link li.fr {
    float: right;
}

.all-right {
    text-align: center;
    line-height: 20px;
    padding-top: 30px;
    color: #a6a6a6;
}

.all-right a {
    color: #a6a6a6;
}


/*当前时间*/

.now-time {
    font-size: 14px;
    color: #333;
    text-align: center;
}

.now-time p {
    padding: 0;
    width: 170px;
}

.now-time .p1 {
    padding: 0;
    font-size: 56px;
    color: #d92028;
}


/*右侧浮层*/

.right-nav {
    position: fixed;
    width: 60px;
    height: 210px;
    position: fixed;
    right: 0;
    top: 50%;
    margin-top: -105px;
    border: 1px solid #e5e5e5;
    border-left: none;
    background: #fff;
    z-index: 100;
}

.right-nav li {
    position: relative;
    height: 60px;
    line-height: 60px;
    text-align: center;
    border-bottom: 1px solid #e5e5e5;
    border-left: 1px solid #e5e5e5;
    cursor: pointer;
    background: #fff;
}

.right-nav li>img {
    vertical-align: middle;
}

.right-nav li:last-child {
    border-bottom: none;
}

.right-nav li.min {
    height: 27px;
    line-height: 27px;
}

.r-sub-nav {
    position: absolute;
    top: -1px;
    right: 59px;
    padding: 10px;
    border: 1px solid #e5e5e5;
    border-right: none;
    display: none;
    background: #f8f8f8;
    width: 138px;
    height: 138px;
}

.r-sub-nav img {
    width: 118px;
    height: 118px;
}

.right-nav li.l:hover {
    border-left: none;
}

.right-nav li:hover .r-sub-nav {
    display: block;
}


/*掌上购样式*/

.zsg-content-wr {
    border-bottom: 1px solid #e5e5e5;
    min-width: 1200px;
}

.zsg-content {
    width: 1078px;
    margin: 0 auto;
    padding: 55px 0;
}

.zsg-content img.down {
    margin-top: 60px;
}


.footer_intro{ text-align:center}

.pages{ clear:both; text-align:center; padding:40px 0; font-size: 12px; }
.pages a{ display:inline-block; height:30px; line-height:30px; text-align:center; margin:0 2px; border:1px solid #e3e3e3;  padding: 0 15px; color: #6a6a6a; }
.pages a:hover{ color: #fe6903; }
.pages .num a.a_cur,.pages .num a:hover{ background:#fe6903; color:#fff; border:1px solid #fe6903;}
.pages.p20{ padding: 20px 0 40px 0; }
.pages .current{background:#ca1622;color:#fff;border:1px solid #fe6903;display: inline-block;height: 30px;line-height: 30px;text-align: center;margin: 0 2px;border: 1px solid #e3e3e3;padding:0 15px;}



