		<script type="text/javascript">
			$(function() {
				$('.freight_company ul').click(function() {
					if($(this).hasClass('on')) {
						$(this).removeClass('on');
						$(this).find('.freight_bottom').attr('src', 'images/bottom.png');
						$(this).parent().find('.freight_desc').addClass('hide');
					} else {
						$(this).addClass('on');
						$(this).find('.freight_bottom').attr('src', 'images/top.png');
						$(this).parent().find('.freight_desc').removeClass('hide');
					}
				});

				$('.area_title span').click(function() {
					$(this).addClass('on').siblings().removeClass('on');
					$('.area_list').eq($(this).index()).removeClass('hide').siblings().addClass('hide');
				});
				
				$('#user_address').click(function(){
					$('.area_info').toggleClass('hide');
				})
				
				$('.area_list span').click(function(){
					$('.area_list span').removeClass('on');
					$(this).addClass('on').siblings().removeClass('on');
					
					$('#user_address').val($(this).html())
				});
			})
		</script>
