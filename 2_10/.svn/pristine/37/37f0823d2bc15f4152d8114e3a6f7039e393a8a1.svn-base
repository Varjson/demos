				$(".muti").on("click", ".plus", function() {
					var v = ($(this).prev().val() == '' ? 0 : Number($(this).prev().val()) + 1)
					$(this).prev().val(v);
				})
				$(".muti").on("click", ".less", function() {
					var v = ($(this).next().val() <= 1 ? 1 : Number($(this).next().val()) - 1)
					$(this).next().val(v);
				})

				function calc_num(obj, type) {
					if(type == 'plus') {
						var v = (obj.prev().val() == '' ? 0 : Number(obj.prev().val()) + 1)
						obj.prev().val(v);
					} else if(type == 'less') {
						var v = (obj.next().val() <= 1 ? 1 : Number(obj.next().val()) - 1)
						obj.next().val(v);
					}
				}

