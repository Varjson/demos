<script type="text/javascript">
	var address_id	=	'';
	function edit_address(v){
		address_id	=	v;
		var global_phone_d	=	'';
		var global_area_d	=	'';
		var province_d	=	'';
		var city_d	=	'';
		$.ajax({
			type: 'post',
			url: "/index.php?c=My&a=get_address",
			dataType: 'json',
			data:{
				address_id	:	v,
			},
			async:false,
			success: function(data){
				if(data.errorCode == 0){
					var data_s	=	data.result.user_address;
					var global_phone_s	=	data.result.global_phone;
					var global_phone_l	=	global_phone_s.length;
					for(var g=0; g<global_phone_l;g++){
						global_phone_d	+=	'<option ';
						if(global_phone_s[g].phone_id == data_s.phone_area){
							global_phone_d	+=	'selected="selected" ';
						}
						global_phone_d	+=	'value="'+global_phone_s[g].phone_id+'">+'+global_phone_s[g].phone_prefix+' '+global_phone_s[g].global_name+'</option>';
					}
					$("#phone_area_s").html(global_phone_d);
					var global_area_s	=	data.result.global_area;
					var global_area_l	=	global_area_s.length;
					for(var q=0; q<global_area_l;q++){
						global_area_d	+=	'<option ';
						if(global_area_s[q].area_id == data_s.country){
							global_area_d	+=	'selected="selected" ';
						}
						global_area_d	+=	'value="'+global_area_s[q].area_id+'">'+global_area_s[q].area_name+'</option>';
					}
					$("#country_s").html(global_area_d);
					if(data.result.province){
						var province_s	=	data.result.province;
						var province_l	=	province_s.length;
						for(var q=0; q<province_l;q++){
							province_d	+=	'<option ';
							if(province_s[q].area_id == data_s.province){
								province_d	+=	'selected="selected" ';
							}
							province_d	+=	'value="'+province_s[q].area_id+'">'+province_s[q].area_name+'</option>';
						}
						$("#province_s").html(province_d);
						$("#province_s").show();

						var city_s	=	data.result.city;
						var city_l	=	city_s.length;
						for(var q=0; q<city_l;q++){
							city_d	+=	'<option ';
							if(city_s[q].area_id == data_s.country){
								city_d	+=	'selected="selected" ';
							}
							city_d	+=	'value="'+city_s[q].area_id+'">'+city_s[q].area_name+'</option>';
						}
						$("#city_s").html(city_d);
						$("#city_s").show();
					}else{
						$("#province_s").hide();
						$("#city_s").hide();
					}
					$("#address_s").val(data_s.address);
					$("#name_s").val(data_s.name);
					$("#phone_s").val(data_s.phone);
					$("#zipcode_s").val(data_s.zipcode);
