	function edit_global(v,vv){
		if(v == 1){
			var province_data	=	'';
			var city_data	=	'';
			$.ajax({
				type: 'post',
				url: "/index.php?c=Area&a=post_province_json",
				dataType: 'json',
				data:{
					area_id	:	v,
				},
				success: function(data){
					if(data.errorCode == 0){
						var province	=	data.result.province;
						var city	=	data.result.city;
						if(province){
							var province_l	=	province.length;
							for(var i=0; i<province_l; i++){
								province_data	+=	'<option value="'+province[i].area_id+'">'+province[i].area_name+'</option>';
							}
							if(vv == 1){
								$("#province").html(province_data);
							}else{
								$("#province_s").html(province_data);
							}
						}
						if(city){
							var city_l	=	city.length;
							for(var u=0; u<city_l; u++){
								city_data	+=	'<option value="'+city[u].area_id+'">'+city[u].area_name+'</option>';
							}
							if(vv == 1){
								$("#city").html(city_data);
								$("#province").show();
								$("#city").show();
							}else{
								$("#city_s").html(city_data);
								$("#province_s").show();
								$("#city_s").show();
							}
						}
					}
				},
				error: function(xhr, type) {
					alert('Ajax error!');
					// 即使加载出错，也得重置
					me.resetload();
				}
			});
		}else{
			if(vv == 1){
				$("#province").hide();
				$("#city").hide();
			}else{
				$("#province_s").hide();
				$("#city_s").hide();
			}
		}
	}
