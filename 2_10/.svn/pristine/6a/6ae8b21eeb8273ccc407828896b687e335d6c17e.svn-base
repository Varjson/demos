.cat_title {
	text-align: center;
}

.cat_title img {
	width: 50px;
	height: 50px;
}

.cat_title span {
	height: 50px;
	line-height: 50px;
	position: absolute;
	top: 5px;
	color: #d92727;
	font-weight: bold;
	padding-left: 20px;
}

.cat_list ul li {
	float: left;
	width: 100px;
	text-align: center;
}

.cat_list .on {
	cursor: pointer;
	font-weight: bold;
	color: #d53939
}

.cat_list {
	padding-bottom: 35px;
	border-bottom: 1px solid #dbdbdb;
}

.cat_list .more {
	display: block;
	float: right;
	cursor: pointer;
	text-decoration: none;
	color: #222
}

.cat_bd ul li img {
	width: 90%;
}

.cat_bd ul li p {
	width: 90%;
	text-align: left;
	padding-left: 5%;
}

.price {
	color: #e11a1a;
	font-weight: bold;
}

.title {
	color: #222
}

.cat_bd ul li {
	display: block;
	float: left;
	width: 25%;
	transition: all .5s linear;
	text-align: center;
	padding-top: 10px;
	cursor: pointer;
	position: relative;
}

.cat_bd ul li .review-wrapper {
	position: absolute;
	bottom: 6px;
	left: 5%;
	z-index: 3;
	width: 90%;
	height: 0;
	overflow: hidden;
	_zoom: 1;
	font-size: 12px;
	background: #df4e4d;
	opacity: 0;
	filter: alpha(opacity=0)\9;
	-webkit-transition: all .2s linear;
	transition: all .2s linear;
}

.cat_bd ul li .review-wrapper a {
	display: block;
	padding: 8px 30px;
	outline: 0;
	text-decoration: none;
}

.cat_bd ul li .review,
.cat_bd ul li .author {
	display: block
}

.cat_bd ul li .review {
	margin-bottom: 5px;
	color: #fff
}

.cat_bd ul li.active .review-wrapper {
	height: 35px;
	opacity: 0.9;
	filter: alpha(opacity=100)\9;
}

.cat_bd ul li.active {
	-webkit-box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
	box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
	-webkit-transform: translate3d(0, -2px, 0);
	transform: translate3d(0, -2px, 0)
}

